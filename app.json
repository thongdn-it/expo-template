{
  "expo": {
    "name": "Expo Template Project",
    "description": "Expo template project\nCreate by Thong Dang.",
    "slug": "expo-template",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 60000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "thongdn.ios.expo-template",
      "buildNumber": "1"
    },
    "android": {
      "package": "thongdn.android.expo_template",
      "versionCode": 1
    }
  }
}
